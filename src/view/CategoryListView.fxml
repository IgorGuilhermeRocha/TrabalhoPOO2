<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox prefHeight="395.0" prefWidth="557.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.CategoryListViewController">
   <children>
      <Label alignment="CENTER" prefHeight="27.0" prefWidth="561.0" text="Lista Categoria">
         <font>
            <Font size="38.0" />
         </font>
      </Label>
      <Button fx:id="btNovo" mnemonicParsing="false" onAction="#onBtNovoAction" text="Novo" />
      <TableView fx:id="tvCategory" prefHeight="315.0" prefWidth="557.0">
        <columns>
          <TableColumn fx:id="tcIdCategory" prefWidth="75.0" text="ID" />
          <TableColumn fx:id="tcName" prefWidth="75.0" text="Nome" />
            <TableColumn fx:id="tableColumnEDIT" prefWidth="75.0" />
            <TableColumn fx:id="tableColumnREMOVE" prefWidth="75.0" />
        </columns>
      </TableView>
   </children>
</VBox>
